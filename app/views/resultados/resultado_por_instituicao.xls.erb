<% content_for :titulo do %>Resultado da <%= @instituicao.nome %><% end %>
<% @pesquisa.dimensoes.each do |dimensao| %>
  <% dimensao.perguntas.each do |pergunta| %>
    <Row>
      <Cell><Data ss:Type="String"><%= pergunta.pergunta %></Data></Cell>
      <Cell ss:StyleID="s64"><Data ss:Type="Number"><%= pergunta.media_por_instituicao(@instituicao) / 100 %></Data></Cell>
    </Row>
  <% end %>
<% end %>
